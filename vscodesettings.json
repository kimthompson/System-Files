{
    "editor.tabSize": 2,
    "editor.fontFamily": "Comic Code, Fira Code, Menlo, Monaco, monospace",
    "editor.fontWeight": "400",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80,
    "editor.fontLigatures": true,
    "breadcrumbs.enabled": true,
    "sasslint.enable": true,
    "sasslint.packageManager": "yarn",
    "prettier.requireConfig": true,
    "terminal.integrated.fontFamily": "Comic Code, Fira Code, Menlo, Monaco, monospace",
    "terminal.integrated.fontWeight": "400",
    "window.zoomLevel": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.enableSmartCommit": true,
    "files.associations": {
        "*.jsx": "javascriptreact",
        "*.js": "javascriptreact"
    },
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "optimizeImages.app": "ImageOptim",
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 80
    },
    "javascript.validate.enable": false,
    "markdown-checkbox.withBulletPoint": true,
    "markdown-checkbox.typeOfBulletPoint": "+",
    "markdown-checkbox.strikeThroughWhenChecked": true,
    "markdown-checkbox.dateWhenChecked": false,
    "editor.minimap.enabled": false,
    "git.autofetch": true,
    "markdown.preview.breaks": true,
    "markdown.extension.toc.unorderedList.marker": "+",
    "markdown.preview.linkify": false,
    "liveshare.featureSet": "insiders",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "workbench.colorTheme": "Eva Dark Bold",
    "explorer.confirmDelete": false,
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "editor.largeFileOptimizations": false,
    "debug.console.fontFamily": "'Comic Code'",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 13,
    "search.location": "panel",
    "debug.toolBarLocation": "docked",
    "window.newWindowDimensions": "maximized",
    "todo-tree.highlights.enabled": true,
    "terminal.external.osxExec": "iTerm.app"
}
